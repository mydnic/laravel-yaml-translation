{
  "name": "jackjoe/yaml-translation",
  "description":
    "Add YAML file support to Laravel TranslationServiceProvider, fork of devitek/yaml-translation",
  "keywords": ["laravel", "translations", "yaml"],
  "license": "MIT",
  "authors": [
    {
      "name": "Jack + Joe",
      "email": "hello@jackjoe.be"
    },
    {
      "name": "Thomas SIEFFERT",
      "email": "caporaldead@devitek.fr"
    }
  ],
  "require": {
    "php": ">=8.1",
    "illuminate/translation": "^10.0",
    "symfony/yaml": "^6.2"
  },
  "autoload": {
    "psr-4": {
      "JackJoe\\": "src/JackJoe"
    }
  },
  "extra": {
    "laravel": {
      "providers": ["JackJoe\\Core\\Translation\\TranslationServiceProvider"]
    }
  }
}
